					"name": "Neutral 100",
					"slug": "neutral-100"
				}
			]
		},
		"custom": {
			"border-radius": {
				"lg": "16px",
				"md": "8px",
				"rounded": "999px",
				"sm": "4px",
				"xl": "32px",
				"zero": "0"
			},
			"border-width": {
				"large": "4px",
				"medium": "3px",
				"small": "2px",
				"tiny": "1px"
			},
			"box-shadow": {
				"1": "0px 2px 8px rgba(33, 33, 33, 0.12)",
				"2": "3px 3px 10px 10px rgba(0, 0, 0, 0.10)"
			},
			"color": {
				"link": "var(--wp--preset--color--accent-80)",
				"neutral-black": "#000000",
				"neutral-white": "#FFFFFF"
			},
			"layout": {
				"block-gap": "2rem",
				"block-gap-large": "40px",
				"content": "1216px",
				"padding": "16px",
				"page": "var(--wp--custom--layout--content)",
				"sidebar": "336px",
				"wide": "90%"
			},
			"letter-spacing": {
				"loose": ".05em",
				"looser": ".1em",
				"none": "normal",
				"tight": "-.01em"
			},
			"line-height": {
				"headings": 1.2,
				"normal": 1.5,
				"tiny": 1
			}
		},
		"layout": {
			"contentSize": "var(--wp--custom--layout--content)",
			"wideSize": "var(--wp--custom--layout--wide)"
		},
		"spacing": {
			"defaultSpacingSizes": false,
			"spacingSizes": [
				{
					"name": "Space 1 (1rem - 16px)",
					"size": "1rem",
					"slug": "10"
				},
				{
					"name": "Space 2 (1.5rem - 24px)",
					"size": "1.5rem",
					"slug": "20"
				},
				{
					"name": "Space 3 (2rem - 32px)",
					"size": "2rem",
					"slug": "30"
				},
				{
					"name": "Space 4 (2.5rem - 40px)",
					"size": "2.5rem",
					"slug": "40"
				},
				{
					"name": "Space 6 (4rem - 64px)",
					"size": "4rem",
					"slug": "60"
				},
				{
					"name": "Space 7 (4.5rem - 72px)",
					"size": "4.5rem",
					"slug": "70"
				},
				{
					"name": "Space 8 (5rem - 80px)",
					"size": "5rem",
					"slug": "80"
				}
			],
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
		"typography": {
			"customFontSize": false,
			"defaultFontSizes": false,
			"dropCap": false,
			"fontFamilies": [
				{
					"fontFace": [
						{
							"fontDisplay": "block",
							"fontFamily": "Delvona",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "bold",
							"src": [
								"file:./assets/fonts/Delvona-ExtraBold.woff2"
							]
						}
					],
					"fontFamily": "\"Delvona\", serif",
					"name": "Heading Font",
					"slug": "font-heading"
				},
				{
					"fontFace": [
						{
							"fontDisplay": "block",
							"fontFamily": "Outfit",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "400 700",
							"src": [
								"file:./assets/fonts/Outfit-VariableFont_wght.ttf"
							]
						}
					],
					"fontFamily": "\"Outfit\", sans-serif",
					"name": "Font main",
					"slug": "font-main"
				}
			],
			"fontSizes": [
				{
					"name": "Display",
					"size": "clamp(3rem, 8.8vw, 8rem)",
					"slug": "display"
				},
				{
					"name": "H1",
					"size": "clamp(3rem, 4.8vw, 4rem)",
					"slug": "heading-1"
				},
				{
					"name": "H2",
					"size": "clamp(2rem, 4.4vw, 2.5rem)",
					"slug": "heading-2"
				},
				{
					"name": "H3",
					"size": "clamp(2rem, 4.4vw, 2.5rem)",
					"slug": "heading-3"
				},
				{
					"name": "H4",
					"size": "clamp(1.5rem, 4.4vw, 1.875rem)",
					"slug": "heading-4"
				},
				{
					"name": "H5",
					"size": "1.25rem",
					"slug": "heading-5"
				},
				{
					"name": "H6",
					"size": "1.125",
					"slug": "heading-6"
				},
				{
					"name": "Subtitle",
					"size": "clamp(0.875rem, 4.4vw, 1.5rem)",
					"slug": "subtitle"
				},
				{
					"name": "Body",
					"size": "clamp(1rem, 2.4vw, 1.125rem)",
					"slug": "body"
				},
				{
					"name": "Small",
					"size": "0.875rem",
					"slug": "small"
				},
				{
					"name": "Caption",
					"size": "0.75rem",
					"slug": "caption"
				}
			],
			"fontWeight": false,
			"letterSpacing": false,
			"lineHeight": false
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"blocks": {
			"core/group": {
				"spacing": []
			}
		},
		"color": {
			"background": "var(--wp--preset--color--background)",
			"text": "var(--wp--preset--color--foreground)"
		},
		"elements": {
			"button": {
				"border": {
					"radius": "var(--wp--custom--border-radius--rounded)",
					"width": "0px"
				},
				"color": {
					"background": "var(--wp--preset--color--primary)",
					"text": "var(--wp--preset--color--foreground)"
				},
				"spacing": {
					"padding": "calc(0.5rem + 4px) 1.5em"
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--body)",
					"lineHeight": "var(--wp--custom--line-height--normal)"
				}
			},
			"h1": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--font-heading)",
					"fontSize": "var(--wp--preset--font-size--heading-1)",
					"lineHeight": "var(--wp--custom--line-height--tiny)"
				}
			},
			"h2": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--font-heading)",
					"fontSize": "var(--wp--preset--font-size--heading-2)"
				}
			},
			"h3": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--font-main)",
					"fontSize": "var(--wp--preset--font-size--heading-3)"
				}
			},
			"h4": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--font-main)",
					"fontSize": "var(--wp--preset--font-size--heading-4)"
				}
			},
			"h5": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--font-main)",
					"fontSize": "var(--wp--preset--font-size--heading-5)"
				}
			},
			"h6": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--font-main)",
					"fontSize": "var(--wp--preset--font-size--heading-6)"
				}
			},
			"heading": {
				"typography": {
					"lineHeight": "var(--wp--custom--line-height--headings)"
				}
			},
			"link": {
				"color": {
					"text": "var(--wp--custom--color--link)"
				}
			}
		},
		"spacing": {
			"blockGap": "var(--wp--custom--layout--block-gap)",
			"padding": {
				"bottom": "0px",
				"left": "2rem",
				"right": "2rem",
				"top": "0px"
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--font-main)",
			"fontSize": "var(--wp--preset--font-size--body)",
			"lineHeight": "var(--wp--custom--line-height--normal)"
		}
	},
	"version": 3
}